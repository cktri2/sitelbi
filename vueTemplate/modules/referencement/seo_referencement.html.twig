<section id="seoReferencement_anchorId" class="moduleDefault _seoReferencementContainer BxAnim animBxFade__Out">
    <div class="moduleDefaultContainer bxrow">
        <div class="imgStructure bxld13 bxt12">
            <img class="image BxAnim animBxFadeBottom__Out" src="/assets/img/modules/referencement/module_seo/seo_img.png" alt="illustration section la boite immo academy">
